@inherits NavMenuBase

<div onkeydown="Navigate">
    <ul class="nav flex-column">

        <li class="nav-item px-3">
            <NavLink class="nav-link button" href="" Match="NavLinkMatch.All">
                <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-circle-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                    <circle cx="8" cy="8" r="8" />
                </svg>
            </NavLink>
        </li>
        @for (int i = 1; i <= 10; i++)
        {
            <li class="nav-item px-3">
                <NavLink class="nav-link button" href="@i">
                    <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-circle-fill" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <circle cx="8" cy="8" r="8" />
                    </svg>
                </NavLink>
            </li>
        }
    </ul>
</div>